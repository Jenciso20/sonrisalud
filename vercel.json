{
  "version": 2,
  "builds": [
    {
      "src": "sonrisalud-frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist/sonrisalud-frontend" }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
