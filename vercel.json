{
  "version": 2,
  "framework": "angular",
  "buildCommand": "cd sonrisalud-frontend && npm install && npm run build",
  "outputDirectory": "sonrisalud-frontend/dist/sonrisalud-frontend",
  "ignoreCommand": "echo 'use frontend only'",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
