{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/sonrisalud-frontend/browser",
  "framework": null,
  "routes": [
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|svg|gif|webp|woff|woff2|ttf|eot|json))",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}